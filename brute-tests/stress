g++ -std=c++17 sol.cpp -o sol
g++ -std=c++17 brute.cpp -o brute
g++ -std=c++17 gen.cpp -o gen

for i in {001..020}; do
  echo "Testing $i..."
  ./gen > "in"
  ./sol < "in" > "solout"
  ./brute < "in" > "bruteout"
  cmp solout bruteout || break
done

rm "solout"
rm "sol"
rm "brute"
rm "bruteout"
rm "gen"